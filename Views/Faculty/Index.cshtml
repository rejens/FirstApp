@model List<FirstApp.Entity.Faculty>

<div class="container">
    <table class="table mt-5 table-dark table-stripped p-2">
        <thead class="table-dark">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Director</th>
            <th>action</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td> @item.Id</td>
                <td> @item.Name</td>
                <td> @item.Director</td>
                <td>
                    @* <a asp-action="Update" asp-route-id="@item.Id" class="btn btn-primary">Edit</a> *@
                    @* <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a> *@
                </td>
            </tr>
        }
        </thead>
    </table>
</div>